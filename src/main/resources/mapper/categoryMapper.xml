<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ykck.mbti.dao.CategoryDao">
<select id="getCategoryList" resultType="com.ykck.mbti.dto.Category">
    select * from Category
</select>

<select id="getCategoryByFirst" resultType= "com.ykck.mbti.dto.Category">
    select * from Category where category_id= #{first_category_id}
</select>

</mapper>